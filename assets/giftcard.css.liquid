}
h6 b em, h5 b em, h4 b em, h3 b em, h2 b em, h1 b em,
h6 strong em,
h5 strong em,
h4 strong em,
h3 strong em,
h2 strong em,
h1 strong em,
h6 em b,
h5 em b,
h4 em b,
h3 em b,
h2 em b,
h1 em b,
h6 em strong,
h5 em strong,
h4 em strong,
h3 em strong,
h2 em strong,
h1 em strong {
  font-style: {{ fonts_type_heading_style_italic }};
}

